<% @articles.each do |article| %>
          <div class="blog-post">
            <h2 class="blog-post-title"><%= article.title %></h2>
            <p class="blog-post-meta">January 1, 2014 by <a href="#">Mark</a></p>
            <p><%= article.text %></p>
            <a><%= link_to 'Show', article_path(article) %></a>
            <a><%= link_to 'Edit', edit_article_path(article) %></a>
        </div><!-- /.blog-post -->
<% end %>